"black--text\">\n        Would you like to delete「{{ item.title }}」??\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer/>\n        <v-btn color=\"grey\" text :disabled=\"loading\" @click=\"onClickClose\">Cancel</v-btn>\n        <v-btn color=\"red\" text :loading=\"loading\" @click=\"onClickDelete\">Delete</v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n<script>\n  import { mapActions } from 'vuex'\n  export default {\n    name: 'DeleteDialog',\n    data () {\n      return {\n        /** dialog indicating situation */\n        show: false,\n        /** loading situation */\n        loading: false,\n        /** received data */\n        item: {}\n      }\n    },\n    methods: {\n      ...mapActions([\n        /** remove the data */\n        'deleteAbData'\n      ]),\n      /**\n      * show the dialog\n      * this method is called from the parent.\n      */\n      open (item) {\n        this.show = true\n        this.item = item\n      },\n      /** when cancell button is clicked */\n      onClickClose () {\n        this.show = false\n      },\n      /** when remove button is clicked. */\n      onClickDelete () {\n        this.deleteAbData({ item: this.item })\n        this.show = false\n      }\n    }\n  }\n</script>\n"],"sourceRoot":"src/components"}]}